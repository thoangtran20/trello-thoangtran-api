	"license": "ISC",
	"bugs": {
		"url": "https://github.com/thoangtran20/trello-thoangtran-api/issues"
	},
	"homepage": "https://github.com/thoangtran20/trello-thoangtran-api#readme",
	"dependencies": {
		"@babel/runtime": "^7.18.3",
		"express": "^4.18.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.18.2",
		"@babel/node": "^7.17.10",
		"@babel/plugin-transform-runtime": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"babel-eslint": "^10.1.0",
